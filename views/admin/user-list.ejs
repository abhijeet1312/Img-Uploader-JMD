<%- include('../partials/header') %>
    <div class="container mt-5">
        <h1>Assignment List </h1>

        <div class="row">
            <div class="col-sm-8">
                <div class="card">
                    <div class="card-body">

                        <% UserslistEJS.forEach(element => { %>
                            <h3>
                                UserName is :
                                <%= element.userName %>.
                            </h3>

                            <a class="btn btn-dark btn-lg" href="/user/<%= element._id%>" role="button">Move to this user page</a>

                            <% }) %>




                    </div>
                </div>
            </div>



        </div>
    </div>

    <%- include('../partials/footer') %>